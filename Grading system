#include<stdio.h>
void main()
{
    int a ;
    printf("\t\t Grading System");
    printf("\nEnter Marks : ");
    scanf("%d",&a);
    if (a>=85 && a<=100)
    {
        printf("\nGrade A")    ;
    }
    else if (a<85 && a >=70)
    {
        printf("\nGrade B")    ;
    }
    else if (a<70 && a >=55)
    {
        printf("\nGrade C")    ;
    }
    else if (a<55 && a >=40)
    {
        printf("\nGrade D")    ;
    }
    else
    {
        printf("\nGrade F");
    } 
    
}
